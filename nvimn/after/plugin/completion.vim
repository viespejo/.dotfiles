" vim: set sw=2 ts=2 sts=2 foldmethod=marker:

if !exists('g:loaded_completion')
  finish
endif

""map <c-space> to manually trigger completion
"imap <silent> <c-space> <Plug>(completion_trigger)
""use <c-l> to switch to previous completion
"imap <c-l> <Plug>(completion_next_source)
""use <c-h> to switch to next completion
"imap <c-h> <Plug>(completion_prev_source)

